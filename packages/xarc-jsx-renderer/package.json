{
  "name": "@xarc/jsx-renderer",
  "version": "0.0.1",
  "description": "Render index.htm from JSX template",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "clap test-only",
    "coverage": "clap check",
    "prepublishOnly": "clap -n build docs && clap check",
    "docs": "clap xarc/docs"
  },
  "keywords": [
    "jsx",
    "renderer",
    "index.htm"
  ],
  "author": "Electrode <https://github.com/electrode-io/electrode>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.7.6",
    "@types/sinon": "^9.0.0",
    "@types/sinon-chai": "^3.2.4",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "@xarc/module-dev": "^2.1.2",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-walmart": "^2.2.1",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-jsdoc": "^21.0.0",
    "mocha": "^7.1.0",
    "munchy": "^1.0.7",
    "nyc": "^15.0.0",
    "prettier": "^2.0.4",
    "sinon": "^7.2.6",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.16",
    "stream-to-array": "^2.3.0",
    "ts-node": "^8.6.2",
    "typedoc": "^0.17.4",
    "typedoc-plugin-external-module-name": "^3.1.0",
    "typescript": "^3.8.3"
  },
  "files": [
    "dist",
    "src"
  ],
  "mocha": {
    "require": [
      "@babel/register",
      "ts-node/register",
      "source-map-support/register",
      "@xarc/module-dev/config/test/setup.js"
    ],
    "recursive": true
  },
  "prettier": {
    "printWidth": 100,
    "arrowParens": "avoid",
    "trailingComma": "none"
  },
  "dependencies": {
    "@xarc/render-context": "^1.0.0",
    "lodash": "^4.17.15",
    "munchy": "^1.0.7",
    "optional-require": "^1.0.0",
    "require-at": "^1.0.4",
    "xaa": "^1.4.0"
  },
  "nyc": {
    "extends": [
      "@istanbuljs/nyc-config-typescript"
    ],
    "all": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "*clap.js",
      "*clap.ts",
      "babel.config.js",
      "coverage",
      "dist",
      "docs",
      "gulpfile.js",
      "test"
    ],
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "cache": false
  },
  "fyn": {
    "dependencies": {
      "@xarc/render-context": "../xarc-render-context",
      "xaa": "../xarc-app/node-modules/xaa"
    },
    "devDependencies": {}
  }
}
