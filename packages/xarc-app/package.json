{
  "name": "@xarc/app",
  "version": "8.1.5",
  "description": "Electrode X application runtime support",
  "main": "arch-clap.js",
  "homepage": "http://www.electrode.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/electrode-io/electrode.git"
  },
  "bugs": {
    "url": "https://github.com/electrode-io/electrode/issues"
  },
  "license": "Apache-2.0",
  "scripts": {
    "test": "clap check",
    "format": "prettier --write --print-width 100 *.{js,jsx} `find . -type d -d 1 -exec echo '{}/**/*.{js,jsx}' \\; | egrep -v '(/node_modules/|/dist/|/coverage/)'`"
  },
  "files": [
    "config",
    "lib",
    "support",
    "arch-clap.js",
    "require.js",
    "typedef.js",
    "supports.js"
  ],
  "author": "Electrode (http://www.electrode.io/)",
  "contributors": [
    "Joel Chen <xchen@walmartlabs.com>"
  ],
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "css-modules-require-hook": "^4.0.2",
    "ignore-styles": "^5.0.1",
    "isomorphic-loader": "^3.1.0",
    "optional-require": "^1.0.0",
    "subapp-util": "^1.0.5"
  },
  "devDependencies": {
    "chalk": "^1.1.3",
    "electrode-archetype-njs-module-dev": "^3.0.0",
    "mock-require": "^3.0.3",
    "prettier": "^1.14.2",
    "prompts": "^2.2.1",
    "request": "^2.88.0",
    "winston": "^2.3.1"
  },
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  },
  "nyc": {
    "all": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "coverage",
      "*clap.js",
      "gulpfile.js",
      "dist",
      "test"
    ]
  },
  "fyn": {
    "dependencies": {
      "subapp-util": "../subapp-util"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "none",
    "arrowParens": "avoid"
  }
}
